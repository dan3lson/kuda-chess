<table class="table table-hover lead">
  <th>

  </th>
  <th>
    White
  </th>
  <th>
    Black
  </th>
  <% @game.moves.each_with_index do |move, num| %>
    <tr>
      <td>
        <%= num + 1 %>.
      </td>
      <td>
        <%= move.white %>
      </td>
      <td>
        <%= move.black %>
      </td>
    </tr>
  <% end %>
</table>
